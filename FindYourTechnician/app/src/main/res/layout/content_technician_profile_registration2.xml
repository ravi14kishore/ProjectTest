<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
app:layout_behavior="@string/appbar_scrolling_view_behavior"
tools:context="com.example.hp.findyourtechnician.technician_profile_registration2"
tools:showIn="@layout/activity_technician_profile_registration2">

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:text="Technician Registration"
    android:id="@+id/LoginContent_TechnicianRegistrationTextView"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Address"
    android:id="@+id/LoginContent_AddressTextView"
    android:layout_below="@+id/LoginContent_TechnicianRegistrationTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="53dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="City"
    android:id="@+id/LoginContent_CityTextView"
    android:layout_marginTop="20dp"
    android:layout_below="@+id/LoginContent_AddressTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="State"
    android:id="@+id/LoginContent_StateTextView"
    android:layout_below="@+id/LoginContent_CityTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="20dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Pincode"
    android:id="@+id/LoginContent_PincodeTextView"
    android:layout_below="@+id/LoginContent_StateTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="20dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Experience"
    android:id="@+id/LoginContent_ExperienceTextView"
    android:layout_below="@+id/LoginContent_PincodeTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="20dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Expertise"
    android:id="@+id/LoginContent_ExpertiseTextView"
    android:layout_below="@+id/LoginContent_ExperienceTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="20dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Base Fare"
    android:id="@+id/LoginContent_BaseFareTextView"
    android:layout_below="@+id/LoginContent_ExpertiseTextView"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="20dp" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_AddressTextField"
    android:layout_below="@+id/LoginContent_TechnicianRegistrationTextView"
    android:layout_toRightOf="@+id/LoginContent_BaseFareTextView"
    android:layout_marginTop="41dp"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_CityTextField"
    android:layout_below="@+id/LoginContent_AddressTextView"
    android:layout_alignLeft="@+id/LoginContent_AddressTextField"
    android:layout_alignStart="@+id/LoginContent_AddressTextField"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_StateTextField"
    android:layout_below="@+id/LoginContent_CityTextView"
    android:layout_alignLeft="@+id/LoginContent_CityTextField"
    android:layout_alignStart="@+id/LoginContent_CityTextField"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_PincodeTextField"
    android:layout_below="@+id/LoginContent_StateTextField"
    android:layout_alignRight="@+id/LoginContent_StateTextField"
    android:layout_alignEnd="@+id/LoginContent_StateTextField"
    android:layout_alignLeft="@+id/LoginContent_StateTextField"
    android:layout_alignStart="@+id/LoginContent_StateTextField" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_ExpertiseTextField"
    android:layout_below="@+id/LoginContent_ExperienceTextView"
    android:layout_alignLeft="@+id/LoginContent_PincodeTextField"
    android:layout_alignStart="@+id/LoginContent_PincodeTextField"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_ExperienceTextField"
    android:layout_below="@+id/LoginContent_PincodeTextView"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_toRightOf="@+id/LoginContent_ExperienceTextView"
    android:layout_toEndOf="@+id/LoginContent_ExperienceTextView" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPersonName"
    android:ems="10"
    android:id="@+id/LoginContent_BaseFareTextField"
    android:layout_below="@+id/LoginContent_ExpertiseTextField"
    android:layout_alignRight="@+id/LoginContent_ExpertiseTextField"
    android:layout_alignEnd="@+id/LoginContent_ExpertiseTextField"
    android:layout_toRightOf="@+id/LoginContent_BaseFareTextView"
    android:layout_toEndOf="@+id/LoginContent_BaseFareTextView" />

<Button
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:text="Register"
    android:id="@+id/LoginContent_RegisterButton"
    android:layout_below="@+id/LoginContent_BaseFareTextField"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="59dp" />

    <ImageView
        android:layout_width="145dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/logotemp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/LoginContent_RegisterButton"
        android:layout_marginTop="35dp"/>

</RelativeLayout>
